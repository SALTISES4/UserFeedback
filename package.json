{
  "name": "ss_kpi",
  "version": "1.0.0",
  "description": "Program Analytics app",
  "main": "gulpfile.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://jsumnerPhD@bitbucket.org/sambhatnagar/ss_kpi.git"
  },
  "author": "Scivero",
  "browserslist": "last 4 versions and >=0.2% in CA and not ie <= 11",
  "license": "UNLICENSED",
  "homepage": "https://bitbucket.org/sambhatnagar/ss_kpi#readme",
  "prettier": {
    "printWidth": 79,
    "parser": "flow",
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": [
      "google",
      "plugin:compat/recommended",
      "plugin:prettier/recommended"
    ],
    "env": {
      "browser": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "strict": 0,
      "arrow-parens": "off",
      "indent": "off",
      "no-invalid-this": "off",
      "no-unexpected-multiline": "off",
      "require-jsdoc": "off",
      "operator-linebreak": "off",
      "guard-for-in": "off",
      "quotes": [
        "error",
        "double"
      ],
      "spaced-comment": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss",
      "stylelint-order"
    ],
    "extends": [
      "stylelint-config-recommended-scss",
      "stylelint-config-prettier"
    ],
    "rules": {
      "order/order": [
        "custom-properties",
        "declarations"
      ],
      "order/properties-alphabetical-order": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.7.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-flow": "^7.0.0",
    "@csstools/postcss-sass": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "browser-sync": "^2.26.7",
    "browsersync": "^0.0.1-security",
    "cssnano": "^4.1.10",
    "gulp": "^4.0.2",
    "gulp-better-rollup": "^4.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rollup": "^2.16.2",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svg-sprite": "^1.5.0",
    "gulp4-run-sequence": "^1.0.0",
    "postcss": "^7.0.23",
    "postcss-preset-env": "^6.7.0",
    "preact": "^10.0.5",
    "preact-material-components": "^1.6.1",
    "prettier": "^1.18.2",
    "rollup": "^1.25.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-uglify": "^6.0.0"
  },
  "dependencies": {
    "core-js": "^3.3.6",
    "regenerator": "^0.14.2"
  }
}
