{
  "name": "django_user_feedback",
  "version": "1.0.0",
  "description": "a simple Django app to collect and store user feedback",
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Scivero",
  "browserslist": "last 4 versions and >=0.2% in CA and not ie <= 11",
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-flow": "^7.10.1",
    "eslint": "^7.3.1",
    "eslint-config-developit": "^1.2.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-preact": "^1.1.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-strip": "^2.0.0",
    "babel-eslint": "^10.0.3",
    "gulp": "^4.0.2",
    "preact": "^10.4.5",
    "prettier": "^2.0.5",
    "rmwc": "4.0.6",
    "rollup": "^2.18.1",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-terser": "^6.1.0",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.3",
    "@babel/runtime-corejs3": "^7.10.4",
    "core-js": "^3.6.5"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "preact",
      "google",
      "prettier"
    ],
    "globals": {
      "gettext": "readonly"
    },
    "parser": "babel-eslint",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "strict": 0,
      "no-invalid-this": "off",
      "arrow-parens": "off",
      "indent": [
        "error",
        2
      ],
      "require-jsdoc": "off",
      "guard-for-in": "off",
      "quotes": [
        "error",
        "double"
      ],
      "radix": "off",
      "react/prefer-stateless-function": "off",
      "spaced-comment": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "prettier/prettier": "error"
    }
  }
}
